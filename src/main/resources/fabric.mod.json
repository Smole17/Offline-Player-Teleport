{
	"schemaVersion": 1,
	"id": "offlineplayerteleport",
	"version": "${version}",
	"name": "Offline Player Teleport",
	"description": "Adds a command that can be used to teleport to players that are currently offline.",
	"authors": [
		"haykam821"
	],
	"contact": {
		"homepage": "https://github.com/haykam821/Offline-Player-Teleport#readme",
		"issues": "https://github.com/haykam821/Offline-Player-Teleport/issues",
		"sources": "https://github.com/haykam821/Offline-Player-Teleport"
	},
	"entrypoints": {
		"main": [
			"io.github.haykam821.offlineplayerteleport.OfflinePlayerTeleport"
		]
	},
	"mixins": [
		"offlineplayerteleport.mixin.json"
	],
	"custom": {
		"modmenu": {
			"links": {
				"modmenu.discord": "https://haykam.com/links/discord"
			}
		}
	},
	"license": "MIT",
	"icon": "assets/offlineplayerteleport/icon.png",
	"depends": {
		"fabricloader": ">=0.4.0",
		"java": ">=17",
		"fabric-command-api-v2": "*",
		"fabric-dimensions-v1": "*",
		"fabric-events-interaction-v0": "*",
		"fabric-resource-loader-v0": "*",
		"fabric-permissions-api-v0": "*",
		"server_translations_api": "*"
	}
}
